#include <stdio.h>
int main() {
    double areaquadrado, a, b, c;
    double areatrianguloretangulo;
    double areatrapezio;
    printf("Digite o numero a: ");
    scanf("%lf", &a);
    printf("Digite o numero b: ");
    scanf("%lf", &b);
    printf("Digite o numero c: ");  
    scanf("%lf", &c);
    areaquadrado = a * a;
    areatrianguloretangulo = (a * b) / 2;
    areatrapezio = ((a + b) * c) / 2;
    printf("A area do quadrado e: %.4lf\n", areaquadrado);
    printf("A area do triangulo retangulo e: %.4lf\n", areatrianguloretangulo);
    printf("A area do trapezio e: %.4lf\n", areatrapezio);
    return 0;
}